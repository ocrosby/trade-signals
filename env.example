# Database Configuration
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/trade_signals"

# Redis Configuration
REDIS_URL="redis://:redis_password@localhost:6379"
REDIS_HOST="localhost"
REDIS_PORT=6379
REDIS_PASSWORD="redis_password"

# API Keys
ALPHA_VANTAGE_API_KEY="your_alpha_vantage_api_key_here"
RAPIDAPI_KEY="your_rapidapi_key_here"
YAHOO_FINANCE_API_KEY="your_yahoo_finance_api_key_here"
COINGECKO_API_KEY="your_coingecko_api_key_here"

# Application Configuration
NODE_ENV="development"
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your_nextauth_secret_here"

# Data Processing
WORKER_MODE="false"
BATCH_SIZE=1000
SYNC_INTERVAL=300000  # 5 minutes in milliseconds

# Cache Configuration
CACHE_TTL=3600  # 1 hour in seconds
REAL_TIME_CACHE_TTL=60  # 1 minute for real-time data

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=900000  # 15 minutes in milliseconds

# Logging
LOG_LEVEL="info"
LOG_FILE="logs/app.log"

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Security
CORS_ORIGIN="http://localhost:3000"
JWT_SECRET="your_jwt_secret_here"
